<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.digind.gasskos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".buttom"
            android:label="@string/title_activity_buttom"></activity>
        <activity
            android:name=".TestingActivity"
            android:label="@string/title_activity_testing"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="id.ac.digind.gasskos.MainActivity" />
        </activity>
        <activity
            android:name=".Verifikasi"
            android:label="Verifikasi Akun Anda"
            android:parentActivityName=".Daftar" />
        <activity
            android:name=".Daftar"
            android:label="Buat Akun Baru"
            android:parentActivityName=".Login" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>